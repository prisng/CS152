object session {
  println("Welcome to the Scala worksheet")

  /********** #1 **********/
  // generic compose combinator
  
	def compose[A, B](f: A => B, g: B => A): B => B = {
		def r(x: B):
			B = f(g(x));
   	r _
	}
	
	def triple(x: Int): Int = 3 * x
	
	def square(x: Int) = x * x
	
	val squareTriple = compose(square_, triple_)
	
	triple(2)
	square(triple(2))
	squareTriple(2)
	
  
